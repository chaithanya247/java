/*non-static method result(int) cannot be referenced from a static context
        result(10);*/

//Error code

class talble{
    int result(int n){
        for(int i=1;i<=10;i++){
            System.out.println(n+"*"+i+"="+i*n);
        }
        return 0;
    }
    public static void main(String args[]){
        result(10);
    }
}
